<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/xforms/">
  <h:head>
    <h:title>Encrypted Form</h:title>
    <model>
      <instance>
        <data id="encrypted-form">
          <name/>
          <image/>
          <orx:meta>
            <orx:instanceID/>
          </orx:meta>
        </data>
      </instance>
      <submission method="form-data-post" base64RsaPublicKey=""/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/name" type="string"/>
      <bind nodeset="/data/image" type="binary"/>
    </model>
  </h:head>
  <h:body>
    <input ref="name">
      <label>Your name</label>
    </input>
    <upload ref="image" mediatype="image/*">
      <label>Take a picture</label>
    </upload>
  </h:body>
</h:html>
